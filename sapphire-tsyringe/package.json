{
	"name": "sapphire-tsyringe",
	"version": "1.0.0",
	"main": "dist/index.js",
	"author": "@sapphire",
	"license": "MIT",
	"dependencies": {
		"@sapphire/decorators": "^5.0.0",
		"@sapphire/discord-utilities": "^2.11.6",
		"@sapphire/discord.js-utilities": "^5.0.1",
		"@sapphire/fetch": "^2.4.1",
		"@sapphire/framework": "^3.1.3",
		"@sapphire/plugin-api": "^4.0.1",
		"@sapphire/plugin-editable-commands": "^2.0.1",
		"@sapphire/plugin-logger": "^3.0.1",
		"@sapphire/plugin-subcommands": "^3.2.2",
		"@sapphire/time-utilities": "^1.7.8",
		"@sapphire/type": "^2.2.4",
		"@sapphire/utilities": "^3.10.1",
		"chokidar": "^3.5.3",
		"concurrently": "^7.6.0",
		"discord.js": "^13.11.0",
		"dotenv-cra": "^3.0.2",
		"nodemon": "^2.0.20",
		"reflect-metadata": "^0.1.13",
		"tsyringe": "^4.7.0"
	},
	"devDependencies": {
		"@sapphire/prettier-config": "^1.4.4",
		"@sapphire/ts-config": "^3.3.4",
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.3.6",
		"@types/node": "^18.8.3",
		"@types/ws": "^8.5.3",
		"prettier": "^2.7.1",
		"tsc-watch": "^5.0.3",
		"typescript": "^4.8.4",
		"vite-tsconfig-paths": "^4.0.3",
		"vitest": "^0.25.8"
	},
	"scripts": {
		"clean": "rm dist -rf",
		"build": "swc src -d dist",
		"watch:build": "swc src -d dist -w",
		"watch:app": "nodemon --watch \"dist/**/**\" -e js .",
		"start": "node dist/index.js",
		"old_dev": "npm-run-all -l clean -p watch:*",
		"dev": "concurrently \"npm run watch:build\" \"npm run watch:app\"",
		"watch:start": "tsc-watch --onSuccess \"node ./dist/index.js\"",
		"test": "vitest --passWithNoTests",
		"test:watch": "vitest watch --passWithNoTests",
		"coverage": "vitest run --coverage",
		"format": "prettier --write \"src/**/*.ts\""
	},
	"prettier": "@sapphire/prettier-config"
}
